{
	"name": "react-router-link-nostack",
	"version": "1.0.7",
	"description": "ðŸŽˆ <Link/> that prevent stacking in browser history upon re-navigating current route",
	"main": "dist/Link.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/tylim88/react-router-link-nostack"
	},
	"scripts": {
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm i",
		"tsc": "tsc -p tsconfig.json --noEmit && npm run dummy",
		"lint": "eslint ./**/*.{tsx,ts} ",
		"lint-fix": "eslint ./**/*.{tsx,ts} --fix",
		"build": "rm -rf dist && rm -rf types && babel src --out-dir dist --extensions \".tsx,.ts\" && tsc -p tsconfig.json",
		"test": "jest",
		"dummy": ""
	},
	"keywords": [
		"react",
		"component",
		"router",
		"link"
	],
	"author": "",
	"license": "MIT",
	"peerDependencies": {
		"react": "^16",
		"react-router-dom": "^5.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.3",
		"@babel/core": "^7.4.3",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@babel/preset-typescript": "^7.10.4",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^9.4.0",
		"@types/jest": "^26.0.4",
		"@types/react-router-dom": "^5.1.5",
		"@types/react-test-renderer": "^16.9.2",
		"@types/styled-components": "^5.1.1",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-root-import": "^6.5.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-config-react-app": "^5.2.0",
		"eslint-plugin-flowtype": "^3.13.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-markdown": "^1.0.2",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"history": "^4.10.1",
		"husky": "^4.2.1",
		"jest": "^25.1.0",
		"jsdoc": "^3.6.5",
		"lint-staged": "^10.2.11",
		"prettier": "^1.19.1",
		"prettier-quick": "0.0.5",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^5.2.1",
		"react-router-dom": "^5.1.2",
		"react-test-renderer": "^16.12.0",
		"styled-components": "^5.0.1",
		"ts-jest": "^26.1.1",
		"typescript": "^3.9.6"
	},
	"dependencies": {},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{tsx,md}": [
			"npm run lint --fix",
			"prettier --write"
		],
		"*.tsx": "npm run tsc"
	},
	"prettier": {
		"arrowParens": "avoid",
		"singleQuote": true,
		"jsxSingleQuote": true,
		"trailingComma": "es5",
		"semi": false,
		"useTabs": true,
		"endOfLine": "auto"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-typescript"
		],
		"plugins": [
			[
				"babel-plugin-root-import",
				{
					"rootPathSuffix": "./src",
					"rootPathPrefix": "~/"
				}
			]
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"prettier/react",
			"eslint:recommended",
			"plugin:prettier/recommended",
			"prettier"
		],
		"plugins": [
			"prettier",
			"markdown",
			"@typescript-eslint"
		],
		"rules": {
			"no-unused-vars": [
				"error",
				{
					"args": "after-used"
				}
			]
		},
		"settings": {
			"import/resolver": {
				"node": {
					"moduleDirectory": [
						"node_modules",
						"src/"
					]
				}
			}
		},
		"overrides": [
			{
				"files": [
					"./**/*.ts"
				],
				"parser": "@typescript-eslint/parser",
				"extends": [
					"plugin:@typescript-eslint/recommended",
					"plugin:prettier/recommended"
				],
				"rules": {
					"@typescript-eslint/member-delimiter-style": 0,
					"@typescript-eslint/camelcase": 0,
					"@typescript-eslint/explicit-function-return-type": 0,
					"@typescript-eslint/explicit-module-boundary-types": 0
				}
			}
		]
	}
}
